using System;

namespace ComparingFloats
{
    class Program
    {
        static void Main(string[] args)
        {
            double a = double.Parse(Console.ReadLine());
            double b = double.Parse(Console.ReadLine());
            bool IsEqual = false;

            if (Math.Abs(a - b) > 0.000001)
            {
                Console.WriteLine(IsEqual);
            }

            else
            {
                IsEqual = true;
                Console.WriteLine(IsEqual);
            }
        }
    }
}
